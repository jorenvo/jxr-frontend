{
  "name": "jxr-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "prebuild": "rm -rvf dist/*",
    "build": "mkdir -p $PWD/dist && tsc && $PWD/post_build.sh",
    "watch-tsc": "tsc --watch",
    "watch": "npm-watch",
    "test": "./prep_tests.sh && mocha -r ts-node/register test_src/*.spec.ts; rm -r $PWD/test_src",
    "env": "env",
    "server-cors": "http-server -c-1 -p 8081 --cors --proxy 'http://127.0.0.1:8000'",
    "server-dist": "http-server -c-1 ./dist"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css",
      "quiet": false
    }
  },
  "keywords": [],
  "author": "Joren Van Onder <joren@jvo.sh>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "http-server": "^13.0.2",
    "npm-watch": "^0.11.0"
  }
}
